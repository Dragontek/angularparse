{
    "compilerOptions": {
      "baseUrl": ".",
      "declaration": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "strictNullChecks": true,
      "noImplicitAny": false,
      "noFallthroughCasesInSwitch": true,
      "paths": {
        "@angular/parse": ["./core"],
        "@angular/parse/auth": ["./auth"],
        "@angular/parse/database": ["./database"],
        "@angular/parse/firestore": ["./firestore"],
        "@angular/parse/functions": ["./functions"],
        "@angular/parse/storage": ["./storage"],
        "@angular/parse/messaging": ["./messaging"],
        "@angular/parse/database-deprecated": ["./database-deprecated"]
      },
      "rootDir": ".",
      "inlineSourceMap": true,
      "lib": ["es5", "dom", "es2015.promise", "es2015.collection", "es2015.iterable"],
      "skipDefaultLibCheck": true,
      "skipLibCheck": true,
      "target": "es2015"
    }
  }